{
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "importOrder": ["^[./]"],
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "es5",
    "printWidth": 100,
    "singleQuote": false,
    "semi": true
}
